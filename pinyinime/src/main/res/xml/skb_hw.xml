<keyboard
    balloon="true"
    height="25%p"
    key_type="0"
    qwerty="true"
    qwerty_uppercase="true"
    repeat="false"
    skb_cache_flag="false"
    skb_template="@xml/skb_template1"
    width="80%p" >

    <row>
  		 <!-- Enter key for hand writing keyboard.-->
	  <key  start_pos_x="75%p" start_pos_y="75%p"
	    width="50%p" height="25%p" code="66" key_type="1" balloon="false">
	    <toggle_state state_id="@string/toggle_enter_go" label="去往" code="66"/>
	    <toggle_state state_id="@string/toggle_enter_search" code="66"
	      icon="@drawable/search_icon" icon_popup="@drawable/search_popup_icon"/>
	    <toggle_state state_id="@string/toggle_enter_send" label="发送" code="66"/>
	    <toggle_state state_id="@string/toggle_enter_next" label="下一个" code="66"/>
	    <toggle_state state_id="@string/toggle_enter_done" label="完成" code="66"/>
	  </key>
        <key
            code="67"
            height="25%p"
            start_pos_x="50%p"
            start_pos_y="0%p"
            width="50%p"
            repeat="true" />
    </row>
    
    
    <row>
        <key
            code="62"
            height="25%p"
            start_pos_x="0%p"
            start_pos_y="25%p"
            width="100%p" />
    </row>
    
    <row>
        <key
            code="-3"
            height="25%p"
            width="100%p"
            start_pos_x="0%p"
            start_pos_y="50%p"
            key_type="2" 
            label="\?123"
         />
    </row>
    
    
    <row>
          
     <key  start_pos_x="0%p" start_pos_y="75%p"
      width="100%p" height="25%p" code="-2" label="中 文" key_type="1"
      repeat="true">
      <toggle_state state_id="@string/toggle_en_lower" label="英 文" code="-2"/>
      <toggle_state state_id="@string/toggle_en_upper" label="英 文" code="-2"/>
      <toggle_state state_id="@string/toggle_hand_writing_text" label="文字手写" code="-2"/>
      <toggle_state state_id="@string/toggle_hand_writing_formula" label="公式手写" code="-2"/>
  	</key>
    </row>


</keyboard>